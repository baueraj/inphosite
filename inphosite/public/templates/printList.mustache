<div class="terms span3">
  <h3>
  {{#alt_title}}
    {{alt_title}}
  {{/alt_title}}
  {{^alt_title}}
    {{attr}}
  {{/alt_title}}

  {{#statistical}}
    <img src="/img/empty.gif" title="List generated by statistical methods." class="fa fa-cog pull-right" width="16">
  {{/statistical}}
  </h3>

  <ol class="unstyled">
    {{#results}}
    <li>
      <a onClick="inpho.tabnav.appendTab('tabnav', 'i{{ID}}', {{label}}, '/entity/{{ID}}/panel/{{parent_id}}')" href=#i{{ID}}" data-toggle="tab" class="tablink"><i class="icon-search"></i></a>
      {{#sep_dir}}
      <a href="http://plato.stanford.edu/entries/{{sep_dir}}"><img src="/img/sepmanicon.png" class="pull-right" /></a>
      {{/sep_dir}}
      {{#wiki}}
      <a href="http://wikipedia.org/wiki/{{wiki}}"><img src="/img/wikiicon.png" class="pull-right" /></a>
      {{/wiki}}
      <a href="{{url}}">{{label}}</a>
    </li>
    {{/results}}
    {{{more}}}
  </ol>
</div>
